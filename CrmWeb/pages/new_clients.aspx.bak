                            <td height="25px">
                                <asp:TextBox runat="server" ID="txtForemanEmail" class="textBoxBorder" size="100" ReadOnly="true"></asp:TextBox>
                            </td>
                        </tr>                       
                    </table>                    
                    <asp:Button runat="server" ID="btnCloseEditForemenPopup" OnClientClick="popupEditForemen.Hide();"
                        Text="Close" />
                </dx:PopupControlContentControl>
            </ContentCollection>
        </dx:ASPxPopupControl>
        </form>
        
    </div>
