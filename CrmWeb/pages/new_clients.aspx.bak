        </asp:DropDownList>
        <asp:Button ID="btnClientFinder" runat="server" OnClick="btnClientFinder_Click" Text="Find"
            Style="margin-right: 20px;" />
        <asp:Button ID="btnNewClient" runat="server" OnClick="btnNewClient_Click" Text="New" />
        <asp:Button ID="btnEditClient" runat="server" OnClick="btnEditClient_Click" Text="Edit" />
        <br />
        <asp:Button ID="Button1" runat="server" Text="Button" OnClientClick="ShowEditSitesPopup();" />
        <dx:ASPxPageControl ID="pageClientData" runat="server" ActiveTabIndex="0" ClientIDMode="AutoID"
            CssFilePath="~/App_Themes/RedWine/{0}/styles.css" CssPostfix="RedWine" Height="336px"
            SpriteCssFilePath="~/App_Themes/RedWine/{0}/sprite.css" TabSpacing="3px" 
            Width="785px">
            <TabPages>
                <dx:TabPage Name="tabClient" Text="Client">
                    <ContentCollection>
                        <dx:ContentControl runat="server" SupportsDisabledAttribute="True">
                            <table>
                                <tr>
                                    <td height="25px">
                                        Client Name:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtNewClientName" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Contact Person:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtNewClientContactPerson" class="textBoxBorder"
                                            size="100" ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Telephone:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtNewClientTelephone" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Cellphone:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtNewClientCellphone" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Email:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtNewClientEmail" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                            </table>
                        </dx:ContentControl>
                    </ContentCollection>
                </dx:TabPage>
                <dx:TabPage Name="tabConsultingEng" Text="Consulting Engineer">
                    <ContentCollection>
                        <dx:ContentControl runat="server" SupportsDisabledAttribute="True">
                            <table>
                                <tr>
                                    <td height="25px">
                                        Firm Name:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtCEFirmName" class="textBoxBorder" size="100" ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Contact Person:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtCEContactPerson" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Telephone:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtCETelephone" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Cellphone:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtCECellphone" class="textBoxBorder" size="100"
                                            ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                                <tr>
                                    <td height="25px">
                                        Email:&nbsp;
                                    </td>
                                    <td height="25px">
                                        <asp:TextBox runat="server" ID="txtCEEmail" class="textBoxBorder" size="100" ReadOnly="true"></asp:TextBox>
                                    </td>
                                </tr>
                            </table>
                        </dx:ContentControl>
                    </ContentCollection>
                </dx:TabPage>
                <dx:TabPage Name="tabProjectManager" Text="Project Manager">
                    <ContentCollection>
                        <dx:ContentControl runat="server" SupportsDisabledAttribute="True">
